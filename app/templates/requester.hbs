<div class="col-md-6 col-md-offset-3">  
  	{{#if responseMessage}}
	<div class="alert alert-success">{{responseMessage}}</div>
	{{/if}}
	<table class="table">
	  <thead>
		<tr>
		  <th colspan="3"><h2>List email are requested</h2></th>
		</tr>
	  </thead>
	  <tbody>
			{{#each model as |request|}}
		  <tr><td width="18%">Email</td><td width="2%">:</td><td>{{request.email}}</td></tr>
			{{/each}}
	  </tbody>
	</table>
</div>  